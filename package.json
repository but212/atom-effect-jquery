{
  "name": "atom-effect-jquery",
  "version": "0.6.0",
  "description": "Reactive jQuery bindings powered by atom-effect",
  "keywords": [
    "jquery",
    "reactivity",
    "atom",
    "signals",
    "state-management",
    "binding",
    "plugin",
    "observer"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/atom-effect-jquery.min.js",
  "jsdelivr": "./dist/atom-effect-jquery.min.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --fix",
    "prepublishOnly": "npm run build"
  },
  "author": "but212",
  "license": "MIT",
  "peerDependencies": {
    "jquery": ">=3.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@but212/atom-effect": "^0.7.0",
    "@types/jquery": "^3.5.33",
    "@vitest/coverage-v8": "^4.0.17",
    "jquery": "^3.7.1",
    "jsdom": "^24.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.17"
  }
}
